<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.user.mapper.ContactMapper">
	<resultMap id="resultContact" type="com.user.bean.Contact" >
		<id column="id" property="id"/>
		<result column="name" property="name"/>
		<result column="linkMans" property="phone" typeHandler="com.user.typeHandler.LinkMansHandler"></result>
	</resultMap>
	<select id="queryContactById" resultMap="resultContact">
		SELECT * FROM CI_CONTACT WHERE ID=#{id};
	</select>
</mapper>
