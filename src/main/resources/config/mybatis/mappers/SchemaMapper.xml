<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mybatis.mapper.SchemaMapper">
	<insert id="createSchema">
		CREATE DATABASE #{schemaName} DEFAULT CHARACTER SET #{charset} COLLATE #{collate};
	</insert>
	<select id="select">
		SELECT 1 FROM dual;
	</select>
</mapper>
